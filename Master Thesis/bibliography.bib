@article{DesignOfThimbleChamber,
doi = {10.1088/0031-9155/17/2/001},
url = {https://dx.doi.org/10.1088/0031-9155/17/2/001},
year = {1972},
month = {mar},
publisher = {},
volume = {17},
number = {2},
pages = {169},
author = {E G A Aird and  F T Farmer},
title = {The design of a thimble chamber for the Farmer dosemeter},
journal = {Physics in Medicine & Biology},
abstract = {By substituting pure graphite and pure aluminium for the present chamber materials a response curve is achieved which is flat over the therapy range and constant from one instrument to another. The response depends critically on dimensions of the inner electrode and this dependence is studied and illustrated. The insulator (amber) is replaced by PTCFE which has a low induced conductivity under radiation and is chemically pure; it is necessary, however, to reduce the area exposed to the air volume on account of its higher atomic number. This reduction also improves precision of the instrument. Dimensions of the final chamber, which is identical externally to the original chamber, are given.}
}
